{
    "license": "MIT",
    "require": {
        "php": "^8.2.0",
        "brick/money": "^0.9.0",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "jcupitt/vips": "^2.4",
        "karlomikus/recipe-utils": "^0.17",
        "kevinrob/guzzle-cache-middleware": "^6.0",
        "laminas/laminas-feed": "^2.23",
        "laravel/cashier-paddle": "^2.0",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.21",
        "laravel/mcp": "^0.3.3",
        "laravel/prompts": "^0.3.4",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.4",
        "laravel/socialite": "^5.18",
        "league/csv": "^9.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "meilisearch/meilisearch-php": "^1.0",
        "nxp/math-executor": "^2.3",
        "promphp/prometheus_client_php": "^2.13",
        "sentry/sentry-laravel": "^4.10",
        "socialiteproviders/authelia": "^4.0",
        "socialiteproviders/authentik": "^5.2",
        "socialiteproviders/keycloak": "^5.3",
        "socialiteproviders/zitadel": "^4.1",
        "spatie/array-to-xml": "^3.1",
        "spatie/laravel-query-builder": "^6.0",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/robots-txt": "^2.5",
        "srwiez/thumbhash": "^1.1",
        "symfony/browser-kit": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/uid": "^7.0",
        "symfony/yaml": "^7.0",
        "wamania/php-stemmer": "^3.0",
        "zircote/swagger-php": "^5.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "driftingly/rector-laravel": "^2.1",
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0",
        "rector/rector": "^2.2"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "Kami\\Cocktail\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "static": "vendor/bin/phpstan analyse",
        "fix-style": "vendor/bin/pint",
        "test": "php artisan test",
        "openapi": "vendor/bin/openapi app -o docs/openapi-generated.yaml --version 3.1.0"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
